.m-header-bar {
  width: 100%;
  background-color: #f8f8f8;
  color: #999999;
  height: 40px;

  .header-content {
    width: 1190px;
    height: 40px;
    margin: 0 auto;
    display: flex;
    align-items: center;
    justify-content: space-between;

    >div:nth-child(1) {
      display: flex;
    }
  }
}